{
    "name": "redpandacoding/contracts",
    "description": "A set of abstractions the are commonly used",
    "keywords": ["abstractions", "contracts", "decoupling", "interfaces", "interoperability", "standards", "dev"],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Redpandacoding\\Contracts\\": "src/"
        },
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "authors": [
        {
            "name": "Jordan Wamser",
            "email": "jwamser@redpandacoding.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4 || ^8.1",
        "psr/container": "^2.0",
        "symfony/service-contracts": "^3.4@dev"
    },
    "conflict": {
        "ext-psr": "<1.1|>=2"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    }
}
